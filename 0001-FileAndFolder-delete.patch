-    }
-
-    LoRaMacClassBHaltBeaconing( );
-
-    MacCtx.MacState |= LORAMAC_TX_RUNNING;
-    if( MacCtx.NodeAckRequested == false )
-    {
-        MacCtx.ChannelsNbTransCounter++;
