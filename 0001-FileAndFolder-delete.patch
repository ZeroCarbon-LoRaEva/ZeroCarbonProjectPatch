-        }
-        case PHY_RECEIVE_DELAY1:
-        {
-            phyParam.Value = IN865_RECEIVE_DELAY1;
-            break;
-        }
-        case PHY_RECEIVE_DELAY2:
-        {
-            phyParam.Value = IN865_RECEIVE_DELAY2;
-            break;
-        }
-        case PHY_JOIN_ACCEPT_DELAY1:
-        {
-            phyParam.Value = IN865_JOIN_ACCEPT_DELAY1;
