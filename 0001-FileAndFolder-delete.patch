-
-    CRITICAL_SECTION_BEGIN( );
-    if (true == timer_flag_expired)
-    {
-        flag = true;
-    }
-    CRITICAL_SECTION_END( );
-
-    return flag;
-}
-
-/***********************************************************************************************************************
