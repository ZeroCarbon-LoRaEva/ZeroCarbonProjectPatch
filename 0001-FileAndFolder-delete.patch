-        outTemp += divResult;
-        in -= divResult * 60;
-        divResult = DIV_APPROX_60( in );
-    }
-    if( in >= 60 )
-    {
-        outTemp += 1;
-        in -= 60;
-    }
-    *remainder = in;
-    *out = outTemp;
-#endif
-}
