-__attribute__ ((section(".ramfunc")))  void agt0_timer_start(uint16_t ms)
-{
-
-    timer_flag_expired = false;
-
-    timer_count = ms;
-
-    /* Start AGT0 count */
-    AGT0->AGTCR_b.TSTART    = 1;
-    
-    /* Wait until the count starts */
-    while(0 == AGT0->AGTCR_b.TCSTF);
-
-    return;
-} /* End of function agt0_timer_start */
