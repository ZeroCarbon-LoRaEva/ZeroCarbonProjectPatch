-{
-  float k = RTC_TEMP_COEFFICIENT;
-  float kDev = RTC_TEMP_DEV_COEFFICIENT;
-  float t = RTC_TEMP_TURNOVER;
-  float tDev = RTC_TEMP_DEV_TURNOVER;
-  float interim = 0.0;
-  float ppm = 0.0;
-  
-  if( k < 0.0 )
-  {
-    ppm = ( k - kDev );
-  }
-  else
-  {
-    ppm = ( k + kDev );
-  }
-  interim = ( temperature - ( t - tDev ) );
-  ppm *=  interim * interim;
