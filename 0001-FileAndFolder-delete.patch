-static void rtc_event_callback(rtc_event_t event)
-{
-    tm_t timeCurrent;
-
-    if ( RTC_EVENT_ALARM == event )
-    {   /* RTC alarm event with 1 second resolution */
-        if ( 0 == rtcAlarm.tm_subsec )
-        {
-            /* Callback the timer IRQ handler if sub-second is zero sharp */
