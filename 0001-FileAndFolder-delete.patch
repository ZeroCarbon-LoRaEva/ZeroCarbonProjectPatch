-static bool VerifyRfFreq( uint32_t freq )
-{
-    // Check radio driver support
-    if( Radio.CheckRfFrequency( freq ) == false )
-    {
-        return false;
-    }
-
-    if( ( freq < 915000000 ) || ( freq > 928000000 ) )
-    {
-        return false;
-    }
-    return true;
-}
