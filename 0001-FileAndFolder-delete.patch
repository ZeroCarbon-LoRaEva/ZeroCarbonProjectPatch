-bool LoRaMacConfirmQueueAdd( MlmeConfirmQueue_t* mlmeConfirm )
-{
-    if( ConfirmQueueCtx.ConfirmQueueNvmCtx->MlmeConfirmQueueCnt >= LORA_MAC_MLME_CONFIRM_QUEUE_LEN )
-    {
-        // Protect the buffer against overwrites
