-***********************************************************************************************************************/
-__attribute__ ((section(".ramfunc")))  static void agt0_timer_isr(void)
-{
-    /* Underflow flag clear */
-    AGT0->AGTCR_b.TUNDF = 0;
-
-    if( 0 == --timer_count )
-    {
-        timer_flag_expired = true;
-    }
-
-} /* End of function agt1_isr */
